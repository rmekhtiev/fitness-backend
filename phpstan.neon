includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app

    # The level 8 is the highest level
    level: 5

    ignoreErrors:
        - '#Unsafe usage of new static#'
        - '#Call to an undefined method Illuminate\\Notifications\\Notification::toSigmaSms\(\)#'

    excludes_analyse:
        - */config/*
        - */database/*
        - */resources/*
        - */routes/*
        - */tests/*

    checkMissingIterableValueType: false
